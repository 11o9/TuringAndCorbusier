<UserControl x:Class="TuringAndCorbusier.xaml.UnitTypeSetting"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TuringAndCorbusier.xaml"
             mc:Ignorable="d" 
             d:DesignWidth="520">
    <Grid>
        <StackPanel x:Name="stackpanel" Background="Gray" Grid.Row="2" HorizontalAlignment="Center" VerticalAlignment="Center" Width="520" Orientation="Vertical" Grid.ColumnSpan="6" Margin="0,0,0,0">
            <Button x:Name="toadd" Height="50" Width="500" HorizontalAlignment="Center" Foreground="Black" Background="DarkGray" Margin="0,5,0,5" Content="타입 추가" Click="toadd_Click" RenderTransformOrigin="0,0"/>
        </StackPanel>

    </Grid>
</UserControl>
