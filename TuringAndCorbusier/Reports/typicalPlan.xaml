<UserControl x:Class="Reports.floorPlanDrawingPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Reports"
        xmlns:mytype="clr-namespace:TuringAndCorbusier"
        mc:Ignorable="d"
         >

    <FixedPage Name="fixedPage" Width="1240" Height="1750">
        <Canvas  Width="1240" Height="1750" Margin="0,10,0,-10">
            <Rectangle Fill="White"/>
            <TextBlock x:Name="planPageTitle" Text="titie"  FontSize="35" Height="50" HorizontalAlignment="Stretch" VerticalAlignment="Top" Width="Auto" Canvas.Left="85" Canvas.Top="80" FontFamily="Myriad Pro" FontWeight="Bold"/>
            <Rectangle Fill="#FF2F9947" Height="5" Canvas.Left="85" Stroke="#FF2F9947" Canvas.Top="120" Width="475" OpacityMask="#FF2F9947"/>

            <Canvas  x:Name="typicalPlanCanvas"  VerticalAlignment="Bottom" Canvas.Left="85" Canvas.Top="185" Width="1075" Height="1410"  >
                <Rectangle Height="1410" Stroke="Black" Width="1075" StrokeThickness="3"/>
            </Canvas>


        </Canvas>
    </FixedPage>
</UserControl>
