<UserControl x:Class="Reports.ImagePage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Reports"
        xmlns:myType ="clr-namespace:TuringAndCorbusier"
        mc:Ignorable="d"
         >

    <FixedPage Name="fixedPage" Width="1240" Height="1750">
        <Canvas  Width="1240" Height="1750">
            <TextBlock Text="2. 조감도"  FontSize="35" Margin="150,150,0,0" Height="50" HorizontalAlignment="Stretch" VerticalAlignment="Top" Width="Auto"/>

            <Canvas x:Name="canvas1" Height="665" Width="940" Margin="150,250,150,0">
                <Canvas.Background>
                    <ImageBrush/>
                </Canvas.Background>
                <Image x:Name="image" Height="645" Canvas.Left="10" Canvas.Top="10" Width="920"/>
            </Canvas>
            <Canvas x:Name="canvas2" Height="665" Width="940" Margin="150,915,150,0">
                <Image x:Name="image1" Height="645" Canvas.Left="10" Canvas.Top="10" Width="920"/>
            </Canvas>
        </Canvas>
    </FixedPage>

</UserControl>
